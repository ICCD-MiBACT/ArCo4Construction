                               rdfs:subClassOf core:Type ;
                               rdfs:comment "Questa classe rappresenta il tipo di un elemento architettonico."@it ,
                                            "This class represents an architectural element type."@en ;
                               rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
                               rdfs:label "Architectural element type"@en ,
                                          "Tipo di elemento architettonico"@en ;
                               owl:versionInfo "stabile"@it ,
                                               "stable"@en .


###  https://w3id.org/arco/ontology/construction-description/ArchitecturalOrder
a-con:ArchitecturalOrder rdf:type owl:Class ;
                         rdfs:subClassOf core:Characteristic ;
                         rdfs:comment "Questa classe rappresenta ognuno dei cinque maggiori stili classici dell'architettura classificata in base allo stile delle colonne, dei capitelli, delle trabeazioni (es. tuscanico, dorico, ionico, corinzio, composito)."@it ,
                                      "This class represents any of the five major classical styles of architecture classified by the style of columns, capitals, entablatures (i.e. Tuscan, Doric, Ionic, Corinthian, Composite)."@en ;
                         rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
                         rdfs:label "Architectural order"@en ,
                                    "Ordine architettonico"@it ;
                         owl:versionInfo "stabile"@it ,
                                         "stable"@en .


###  https://w3id.org/arco/ontology/construction-description/BarrierElement
a-con:BarrierElement rdf:type owl:Class ;
                     rdfs:subClassOf a-con:ArchitecturalElement ;
                     owl:disjointWith a-con:OpeningElement .


###  https://w3id.org/arco/ontology/construction-description/Bay
a-con:Bay rdf:type owl:Class ;
          rdfs:subClassOf a-con:ConstructionSpace ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty core:hasType ;
                            owl:allValuesFrom a-con:BaysType
                          ] ;
          rdfs:comment "Questa classe rappresenta una campata, cioè lo spazio ripetuto di un edificio o di una parte di esso, definito da elementi architettonici ricorrenti, come archi, volte, aperture o pilastri."@it ,
                       """This class represents a bay of a construction. 
I.e: the repeated, similar divisions or compartments of a building or part of a building, as defined by recurring architectural features, such as arches, vaults, openings, or pilasters."""@en ;
          rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
          rdfs:label "Bay"@en ,
                     "Campata"@it ;
          owl:versionInfo "instabile"@it ,
                          "unstable"@en .


###  https://w3id.org/arco/ontology/construction-description/BaysType
a-con:BaysType rdf:type owl:Class ;
               rdfs:subClassOf a-con:ConstructionSpaceType ;
               rdfs:comment "Questa classe rappresenta un tipo di configurazione delle campate di uno spazio, ad es. campate alternate, campate regolari."@it ,
                            """This class represents a type of configuration of the bays of a space. 
e.g. alternating bays, regular bays, etc."""@en ;
               rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
               rdfs:label "Bay type"@en ,
                          "Tipo di campate"@it ;
               owl:versionInfo "instabile"@it ,
                               "unstable"@en .


###  https://w3id.org/arco/ontology/construction-description/Beam
a-con:Beam rdf:type owl:Class ;
           rdfs:subClassOf a-con:HorizontalElement ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty a-con:encastrated ;
                             owl:allValuesFrom xsd:boolean
                           ] ;
           rdfs:comment "Questa classe rappresenta una trave"@it ,
                        "This class represents a beam"@en ;
           rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
           rdfs:label "Beam"@en ,
                      "Trave"@it ;
           owl:versionInfo "stabile"@it ,
                           "stable"@en .


###  https://w3id.org/arco/ontology/construction-description/Building
a-con:Building rdf:type owl:Class ;
               rdfs:subClassOf a-con:ConstructionSpace ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty a-con:numberOfFloors ;
                                 owl:allValuesFrom rdfs:Literal
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty a-con:numberOfRooms ;
                                 owl:allValuesFrom xsd:positiveInteger
