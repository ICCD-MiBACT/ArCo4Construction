               rdfs:comment "Questa classe rappresenta un tipo di costruzione, definito dalla presenza di muri e di una copertura."@it ,
                            "This class represents a kind of construction, defined by walls and a roof."@en ;
               rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
               rdfs:label "Building"@en ,
                          "Edificio"@it ;
               owl:versionInfo "stabile"@it ,
                               "stable"@en ;
               a-cat:iccdNormTag "Normativa A: SI/SII/SIIN;SIIP" .


