###  https://w3id.org/arco/ontology/construction-description/hasArchitecturalOrder
a-con:hasArchitecturalOrder rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf a-dd:hasTechnicalCharacteristic ;
                            owl:inverseOf a-con:isArchitecturalOrderOf ;
                            rdfs:domain a-con:VerticalElement ;
                            rdfs:range a-con:ArchitecturalOrder ;
                            rdfs:comment "Questa proprietà collega un'entità all'ordine architettonico."@it ,
                                         "This property connects an entity to the architectural order."@en ;
                            rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
                            rdfs:label "ha ordine architettonico"@it ,
                                       "has architectural order"@en ;
                            owl:versionInfo "stabile"@en ,
                                            "stable"@en .


###  https://w3id.org/arco/ontology/construction-description/hasConstructionDevelopment
a-con:hasConstructionDevelopment rdf:type owl:ObjectProperty ;
                                 owl:inverseOf a-con:isConstructionDevelopmentOf ;
                                 rdfs:domain a-con:ConstructionSpace ;
                                 rdfs:range a-con:ConstructionDevelopment ;
                                 rdfs:comment "Questa proprietà collega una costruzione allo sviluppo interno ."@it ,
                                              "This property connects a construction to the internal development."@en ;
                                 rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
                                 rdfs:label "ha sviluppo della costruzione"@it ,
                                            "has construction development"@en ;
                                 owl:versionInfo "stabile"@it ,
                                                 "stable"@en ;
                                 a-cat:iccdNormTag "ICCD Normativa A: SI/SII" .


