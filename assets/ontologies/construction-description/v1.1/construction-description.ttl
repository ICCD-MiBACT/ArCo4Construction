###  https://w3id.org/arco/ontology/construction-description/Construction
a-con:Construction rdf:type owl:Class ;
                   rdfs:subClassOf l0:Object ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasConstructionDevelopment ;
                                     owl:allValuesFrom a-con:ConstructionDevelopment
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasConstructionElement ;
                                     owl:allValuesFrom a-con:ConstructionElement
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasConstructionSpace ;
                                     owl:allValuesFrom a-con:ConstructionSpace
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasCovering ;
                                     owl:allValuesFrom a-con:Covering
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasModeOfSuperelevation ;
                                     owl:allValuesFrom a-con:SuperelevationMode
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasPositionOnCeilingLevel ;
                                     owl:allValuesFrom a-con:PositionOnCeilingLevel
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:hasPositionOnGroundLevel ;
                                     owl:allValuesFrom a-con:PositionOnGroundLevel
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-dd:hasMeasurementCollection ;
                                     owl:allValuesFrom a-dd:MeasurementCollection
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-dd:hasOrientation ;
                                     owl:allValuesFrom a-dd:Orientation
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-dd:hasTechnicalStatus ;
                                     owl:allValuesFrom a-dd:TechnicalStatus
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:isolatedElementOrSpace ;
                                     owl:allValuesFrom xsd:boolean
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:jutting ;
                                     owl:allValuesFrom xsd:boolean
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty a-con:withoutOpening ;
                                     owl:allValuesFrom xsd:boolean
                                   ] ;
                   rdfs:comment "Any work or artefact, which can be permanently fixed to the ground or which contributes to defining a more complex work/artefact, created through a process involving the construction of a set of connected structures on the basis of a project or planning level."@en ,
                                "Qualsiasi opera o manufatto, che può essere stabilmente fissata al suolo o che contribuisce a definire un'opera/manufatto più complessa, realizzata mediante un processo che coinvolge l'edificazione di un insieme di strutture collegate sulla base di un progetto o livello di pianificazione."@it ;
                   rdfs:isDefinedBy <https://w3id.org/arco/ontology/construction-description> ;
                   rdfs:label "Construction"@en ,
                              "Costruzione"@it ;
                   owl:versionInfo "stabile"@it ,
                                   "stable"@en .


###  https://w3id.org/arco/ontology/construction-description/ConstructionDevelopment
